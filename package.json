{
  "name": "whitecat-bot",
  "version": "4.0.0",
  "description": "WhiteCat Discord Bot - Remake with modular architecture",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:bot": "ts-node src/bot/index.ts",
    "dev:web": "ts-node src/web/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "start:bot": "node dist/bot/index.js",
    "start:web": "node dist/web/index.js",
    "db:init": "ts-node src/database/init.ts init",
    "db:drop": "ts-node src/database/init.ts drop",
    "db:reset": "ts-node src/database/init.ts reset",
    "db:seed": "ts-node src/database/seed.ts",
    "db:seed:all": "ts-node src/database/seed.ts all",
    "db:seed:ports": "ts-node src/database/seed.ts ports",
    "db:seed:pricing": "ts-node src/database/seed.ts pricing"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "postgresql"
  ],
  "author": "GauCandy",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "rimraf": "^5.0.5"
  }
}
