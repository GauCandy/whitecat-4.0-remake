{
  "name": "whitecat-hosting-bot",
  "version": "4.0.0",
  "description": "WhiteCat Discord Bot - Hosting Management with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && mkdir -p dist/locales && cp src/locales/*.json dist/locales/",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "ts-node src/scripts/deploy-commands.ts",
    "deploy:guild": "ts-node src/scripts/deploy-commands.ts guild",
    "deploy:global": "ts-node src/scripts/deploy-commands.ts global",
    "clear:guild": "ts-node src/scripts/clear-commands.ts guild",
    "clear:global": "ts-node src/scripts/clear-commands.ts global",
    "archive:prod": "npm run build && ts-node src/scripts/archive.ts prod",
    "archive:source": "ts-node src/scripts/archive.ts source",
    "db:init": "ts-node src/database/init.ts init",
    "db:drop": "ts-node src/database/init.ts drop",
    "db:reset": "ts-node src/database/init.ts reset",
    "db:cleanup": "ts-node src/database/init.ts cleanup",
    "guilds:stats": "ts-node src/scripts/cleanup-guilds.ts",
    "guilds:cleanup": "ts-node src/scripts/cleanup-guilds.ts --cleanup"
  },
  "keywords": [
    "discord",
    "bot",
    "hosting",
    "typescript",
    "postgresql"
  ],
  "author": "GauCandy",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "archiver": "^7.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
